<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Background"
    tools:context=".ui.food_in_formation.FoodInformation">

    <androidx.viewpager2.widget.ViewPager2
        android:id="@+id/viewpager2FoodInformation"
        android:layout_width="@dimen/food_information_viewpager2_width"
        android:layout_height="@dimen/food_information_viewpager2_height"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <me.relex.circleindicator.CircleIndicator3
        android:id="@+id/CircleIndicator3_Image_Food"
        android:layout_width="@dimen/food_information_CircleIndicator3_width"
        android:layout_height="@dimen/food_information_CircleIndicator3_height"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewpager2FoodInformation" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/food_information_text_view_margin_10"
        android:text="@string/food_information_title"
        android:textAppearance="@style/Overlay.FoodInformation.Title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CircleIndicator3_Image_Food" />
    <TextView
        android:id="@+id/textView9"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/food_information_text_view_margin_40"
        android:layout_marginTop="@dimen/food_information_text_view_margin_50"
        android:layout_marginEnd="@dimen/food_information_text_view_margin_40"
        android:text="@string/food_information_title1"
        android:textAppearance="@style/Overlay.FoodInformation.Title1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/food_information_text_view_margin_40"
        android:layout_marginTop="@dimen/food_information_text_view_margin_10"
        android:layout_marginEnd="@dimen/food_information_text_view_margin_40"
        android:text="@string/food_information_title3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/food_information_text_view_margin_40"
        android:layout_marginTop="@dimen/food_information_text_view_margin_30"
        android:layout_marginEnd="@dimen/food_information_text_view_margin_40"
        android:text="@string/food_information_title2"
        android:textAppearance="@style/Overlay.FoodInformation.Title1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

</androidx.constraintlayout.widget.ConstraintLayout>