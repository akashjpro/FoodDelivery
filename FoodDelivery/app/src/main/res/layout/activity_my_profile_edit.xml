<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="ViewModel"
            type="com.its.food.delivery.ui.my_profile2.MyProfileEditViewModel" />
    </data>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/Background"
        tools:context=".ui.my_profile2.MyProfileEditActivity">
        <com.google.android.material.appbar.MaterialToolbar
            android:id="@+id/toolbarMyProfileEdit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:navigationIcon="@drawable/ic_vector_back"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toolbarMyProfileEdit">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_constraintStart_toStartOf="parent">

                <TextView
                    android:id="@+id/textView26"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/check_out_margin_50"
                    android:layout_marginTop="@dimen/check_out_margin_32"
                    android:layout_marginEnd="@dimen/check_out_margin_50"
                    android:text="@string/my_profile_2_title"
                    android:textAppearance="@style/Overlay.checkout.Title"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView27"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/m2_margin_50"
                    android:layout_marginLeft="@dimen/m2_margin_50"
                    android:layout_marginTop="@dimen/m2_margin_30"
                    android:text="@string/my_profile_2_title1"
                    android:textAppearance="@style/Overlay.MyProfile.Title"
                    app:layout_constraintEnd_toStartOf="@+id/textView28"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView26" />

                <TextView
                    android:id="@+id/textView28"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/m2_margin_50"
                    android:layout_marginRight="@dimen/m2_margin_50"
                    android:text="@string/my_profile_2_change"
                    android:textAppearance="@style/Overlay.checkout.Title2"
                    app:layout_constraintBottom_toBottomOf="@+id/textView27"
                    app:layout_constraintEnd_toEndOf="parent" />

                <ImageView
                    android:id="@+id/imageFramView1"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/m2_image_information_frame_height"
                    android:layout_marginStart="@dimen/m2_margin_40"
                    android:layout_marginTop="@dimen/m2_margin_15"
                    android:layout_marginEnd="@dimen/m2_margin_40"
                    android:src="@drawable/shape_rectangle_while1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView28" />

                <ImageView
                    android:id="@+id/imageViewUser"
                    android:layout_width="@dimen/m_imageViewUser_width"
                    android:layout_height="@dimen/m_imageViewUser_height"
                    android:layout_marginStart="@dimen/m2_margin_20"
                    android:layout_marginLeft="@dimen/m2_margin_20"
                    android:layout_marginTop="@dimen/m2_margin_20"
                    android:layout_weight="1"
                    android:src="@drawable/img_rectangle_6"
                    app:layout_constraintStart_toStartOf="@+id/imageFramView1"
                    app:layout_constraintTop_toTopOf="@+id/imageFramView1" />

                <TextView
                    android:id="@+id/tvProfileName"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/m2_margin_15"
                    android:layout_marginLeft="@dimen/m2_margin_15"
                    android:layout_marginTop="@dimen/m2_margin_20"
                    android:text="@string/my_profile_name"
                    android:textAppearance="@style/Overlay.MyProfile.Name"
                    app:layout_constraintEnd_toStartOf="@+id/imgbtnEditProfile"
                    app:layout_constraintStart_toEndOf="@+id/imageViewUser"
                    app:layout_constraintTop_toTopOf="@+id/imageFramView1" />


                <TextView
                    android:id="@+id/tvProfileEmail"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/m2_margin_15"
                    android:layout_marginLeft="@dimen/m2_margin_15"
                    android:layout_marginTop="@dimen/m_tv_margin_10"
                    android:layout_marginEnd="@dimen/m2_margin_20"
                    android:layout_marginRight="@dimen/m2_margin_20"
                    android:text="@string/my_profile_email"
                    app:layout_constraintEnd_toEndOf="@+id/imageFramView1"
                    app:layout_constraintStart_toEndOf="@+id/imageViewUser"
                    app:layout_constraintTop_toBottomOf="@+id/tvProfileName" />

                <TextView
                    android:id="@+id/tvProfilePhoneNumber"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/m2_margin_15"
                    android:layout_marginLeft="@dimen/m2_margin_15"
                    android:layout_marginTop="@dimen/m_tv_margin_10"
                    android:layout_marginEnd="@dimen/m2_margin_20"
                    android:layout_marginRight="@dimen/m2_margin_20"
                    android:text="+234 9011039271"
                    app:layout_constraintEnd_toEndOf="@id/imageFramView1"
                    app:layout_constraintStart_toEndOf="@+id/imageViewUser"
                    app:layout_constraintTop_toBottomOf="@+id/tvProfileEmail" />

                <TextView
                    android:id="@+id/tvProfileAddress"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/m2_margin_15"
                    android:layout_marginLeft="@dimen/m2_margin_15"
                    android:layout_marginTop="@dimen/m_tv_margin_10"
                    android:layout_marginEnd="@dimen/m2_margin_20"
                    android:layout_marginRight="@dimen/m2_margin_20"
                    android:text="@string/my_profile_address"
                    app:layout_constraintEnd_toEndOf="@id/imageFramView1"
                    app:layout_constraintStart_toEndOf="@+id/imageViewUser"
                    app:layout_constraintTop_toBottomOf="@+id/tvProfilePhoneNumber" />

                <ImageView
                    android:id="@+id/imgOrders"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/m2_image_information_frame_1_height"
                    android:layout_marginStart="@dimen/m2_margin_40"
                    android:layout_marginTop="@dimen/m2_margin_20"
                    android:layout_marginEnd="@dimen/m2_margin_40"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageFramView1"
                    app:srcCompat="@drawable/shape_rectangle_while6" />

                <TextView
                    android:id="@+id/tvOrders"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/m2_margin_20"
                    android:layout_marginEnd="@dimen/m2_margin_20"
                    android:drawableRight="@drawable/ic_vector__1"
                    android:onClick="@{()->ViewModel.onClickimgOrders()}"
                    android:text="@string/my_profile_2_orders"
                    android:textAppearance="@style/Overlay.MyProfile.Title"
                    app:layout_constraintBottom_toBottomOf="@+id/imgOrders"
                    app:layout_constraintEnd_toEndOf="@+id/imgOrders"
                    app:layout_constraintStart_toStartOf="@+id/imgOrders"
                    app:layout_constraintTop_toTopOf="@+id/imgOrders" />

                <ImageView
                    android:id="@+id/imgPending"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/m2_image_information_frame_1_height"
                    android:layout_marginStart="@dimen/m2_margin_40"
                    android:layout_marginTop="@dimen/m2_margin_20"
                    android:layout_marginEnd="@dimen/m2_margin_40"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imgOrders"
                    app:srcCompat="@drawable/shape_rectangle_while6" />

                <TextView
                    android:id="@+id/tvPending"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/m2_margin_20"
                    android:layout_marginEnd="@dimen/m2_margin_20"
                    android:drawableRight="@drawable/ic_vector__1"
                    android:text="@string/my_profile_2_Pending_reviews"
                    android:textAppearance="@style/Overlay.MyProfile.Title"
                    app:layout_constraintBottom_toBottomOf="@+id/imgPending"
                    app:layout_constraintEnd_toEndOf="@+id/imgPending"
                    app:layout_constraintStart_toStartOf="@+id/imgPending"
                    app:layout_constraintTop_toTopOf="@+id/imgPending" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/m2_image_information_frame_1_height"
                    android:layout_marginStart="@dimen/m2_margin_40"
                    android:layout_marginTop="@dimen/m2_margin_20"
                    android:layout_marginEnd="@dimen/m2_margin_40"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imgPending"
                    app:srcCompat="@drawable/shape_rectangle_while6" />

                <TextView
                    android:id="@+id/textView31"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/m2_margin_20"
                    android:layout_marginEnd="@dimen/m2_margin_20"
                    android:drawableRight="@drawable/ic_vector__1"
                    android:text="@string/my_profile_2_Faq"
                    android:textAppearance="@style/Overlay.MyProfile.Title"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView8"
                    app:layout_constraintEnd_toEndOf="@+id/imageView8"
                    app:layout_constraintStart_toStartOf="@+id/imageView8"
                    app:layout_constraintTop_toTopOf="@+id/imageView8" />

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/m2_image_information_frame_1_height"
                    android:layout_marginStart="@dimen/m2_margin_40"
                    android:layout_marginTop="@dimen/m2_margin_20"
                    android:layout_marginEnd="@dimen/m2_margin_40"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView8"
                    app:srcCompat="@drawable/shape_rectangle_while6" />

                <TextView
                    android:id="@+id/textView32"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/m2_margin_20"
                    android:layout_marginEnd="@dimen/m2_margin_20"
                    android:drawableRight="@drawable/ic_vector__1"
                    android:text="@string/my_profile_2_Help"
                    android:textAppearance="@style/Overlay.MyProfile.Title"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView9"
                    app:layout_constraintEnd_toEndOf="@+id/imageView9"
                    app:layout_constraintStart_toStartOf="@+id/imageView9"
                    app:layout_constraintTop_toTopOf="@+id/imageView9" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/btnUpdate"
                    android:layout_width="@dimen/button_width"
                    android:layout_height="@dimen/button_height"
                    android:layout_marginStart="@dimen/m2_margin_50"
                    android:layout_marginTop="@dimen/margin_40"
                    android:layout_marginEnd="@dimen/m2_margin_50"
                    android:layout_marginBottom="@dimen/m2_margin_40"
                    android:text="@string/my_profile_button_update"
                    android:textAppearance="@style/Overlay.MyProfile.Button_Update"
                    app:cornerRadius="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView9" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.core.widget.NestedScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>